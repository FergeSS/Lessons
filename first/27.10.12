#include <iostream>

int main(){
    int m, n;
    int count = 1;
    
    std::cin >> m >> n;
    std::cout << std::endl;
    std::cout << std::endl;
    
    int** arr_2d = new int*[m];
    
    for (int i=0; i<m; ++i)
        arr_2d[i] = new int[n];
    
    for (int i=0; i<m;++i){
        for (int k=0; k<n;++k){
            arr_2d[i][k] = count;
            ++count; 
        }
    }
    count = 1;
    
    for (int i=0; i<m;++i){
        for (int k=0; k<n;++k)
            std::cout << arr_2d[i][k] << " ";
        std::cout << std::endl;
    }
    
    for (int i = 0; i<m; ++i)
        delete []arr_2d[i];
    delete []arr_2d;
    
    std::cout << std::endl;
    
    arr_2d = new int*[n];
    
    for (int i=0; i<n; ++i)
        arr_2d[i] = new int[m];
    
    for (int i=0; i<n;++i){
        for (int k=0; k<m;++k){
            int count_copy = count
            arr_2d[i][k] = count_copy;
            count_copy += n; 
        }
        ++count; 
    }
    
      for (int i=0; i<n;++i){
        for (int k=0; k<m;++k)
            std::cout << arr_2d[i][k] << " ";
        std::cout << std::endl;
    }
    return 0;
}
